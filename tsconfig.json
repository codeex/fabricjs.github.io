{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "strictNullChecks": true,
    "allowJs": true,
    "noUnusedLocals":false,
    "noUnusedParameters":false,
  },
  "exclude": ["fabric.js", "src/content/", "dist", "**/*.astro"],
}