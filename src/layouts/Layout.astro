---
import Header from '../components/Header/Header.astro';
import './style.css';
import './colorvars.css';
import CarbonAd from '../components/CarbonAd/InlineCarbonAd';
import Footer from '../components/Footer/Footer.astro';
interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="zh">
	<head>
		<meta charset="UTF-8" />
		<meta name="description" content="Astro description" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/favicon.ico" />
		<link rel="sitemap" href="/sitemap-index.xml" />
		<meta name="author" content="webmote,webmote@gmail.com" />
		<meta name="copyright" content="版权所有2024-2030 webmote,codeex" />
		<meta
			name="keywords"
			content="fabric,fabricjs,fabricjs.com,canvas,javascript,vue,react,绘图,画板,类库,游戏,流程图,flowchart,动画,画布,教程,学习"
		/>
		<meta name="generator" content={Astro.generator} />
		<title>{title}</title>
	</head>
	<body>
		<div id="main-content"></div>
		<Header />
		<!-- <CarbonAd client:load /> -->
		<slot />
		<Footer />
	</body>
</html>
